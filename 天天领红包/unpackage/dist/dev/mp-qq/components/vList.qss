.this-list {
  padding: 20rpx;
}
.this-list .this-title {
  font-size: 24rpx;
  color: #999;
  margin-bottom: 10rpx;
}
.this-list .this-item {
  margin-bottom: 40rpx;
}
.this-list .this-body {
  padding: 35rpx 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  border-bottom: 1rpx solid #eee;
}
.this-list .this-body .this-info {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.this-list .this-body .this-info image {
  width: 52rpx;
  height: 52rpx;
  margin-right: 24rpx;
}
.this-list .this-body .this-info view {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}
.this-list .this-body .this-info view text {
  font-size: 28rpx;
  font-weight: 500;
  color: #333;
}
.this-list .this-body .this-info view text+text {
  font-size: 24rpx;
  color: #999;
  margin-top: 10rpx;
}
.this-list .this-body .this-num {
  font-size: 40rpx;
  font-weight: bold;
  color: #333;
}
.no-data {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  height: 80vh;
}
.no-data image {
  width: 231rpx;
  height: 186rpx;
}
.no-data text {
  font-size: 28rpx;
  color: #999;
  margin-top: 30rpx;
}

