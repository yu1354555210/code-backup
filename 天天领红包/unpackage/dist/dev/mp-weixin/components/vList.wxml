<view class="this-list"><block wx:for="{{dataList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="this-item"><view class="this-title">{{item.title}}</view><block wx:for="{{item.info}}" wx:for-item="it" wx:for-index="idx" wx:key="idx"><view class="this-body"><view class="this-info"><image src="{{'https://app1-api.qianr.com/static/bonus/icon-'+iconName+'.png'}}" mode="aspectFill"></image><view><block wx:if="{{iconName=='cash'}}"><text>账户余额提现-到微信零钱</text></block><block wx:else><block wx:if="{{iconName=='review'}}"><text>提现申请-审核中</text></block><block wx:else><text>{{it.total+"元红包"}}</text></block></block><text>{{it.time}}</text></view></view><text class="this-num">{{it.money}}</text></view></block></view></block><block wx:if="{{dataList.length===0}}"><view class="no-data"><text>暂无数据</text></view></block></view>