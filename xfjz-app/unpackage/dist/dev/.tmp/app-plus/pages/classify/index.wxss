.search-bar {
  width: 100%;
  height: 88rpx;
  background: #ffffff;
  box-shadow: 0px 3px 7px 0px rgba(104, 154, 255, 0.35);
  border-radius: 44px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin-bottom: 30rpx;
}
.search-bar image {
  width: 36rpx;
  height: 36rpx;
  margin-right: 10rpx;
}
.search-bar text {
  color: #999999;
  font-size: 26rpx;
}

