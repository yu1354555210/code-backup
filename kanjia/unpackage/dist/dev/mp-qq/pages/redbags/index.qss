.content {
  padding: 15rpx;
}
.content .redbags {
  width: 718rpx;
  height: 324rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  border-radius: 20rpx;
  position: relative;
  padding: 30rpx;
}
.content .redbags .redbags-bg {
  position: absolute;
  z-index: -1;
  height: 100%;
  left: 0;
  top: 0;
}
.content .redbags-fl {
  font-size: 30rpx;
  color: #fff;
}
.content .redbags-fl .redbags-num {
  margin-top: 30rpx;
}
.content .redbags-fl .redbags-num text {
  font-size: 80rpx;
}
.content .redbags-fr image {
  width: 200rpx;
  height: 60rpx;
}
.redbags-rules {
  margin-top: 30rpx;
}
.redbags-rules .redbags-title image {
  width: 254rpx;
  height: 35rpx;
}
.redbags-rules .redbags-rules-list {
  font-size: 26rpx;
  margin-top: 20rpx;
}
.redbags-rules .redbags-rules-list text {
  display: block;
  margin-bottom: 20rpx;
}

