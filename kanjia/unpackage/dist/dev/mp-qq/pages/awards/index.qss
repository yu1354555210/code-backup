.content {
  padding: 15rpx;
}
.content .award-item {
  position: relative;
  padding: 30rpx;
  height: 270rpx;
  margin-top: 30rpx;
  background-image: url(data:image/png;base64,);
  background-size: 719rpx 100%;
  background-repeat: no-repeat;
}
.content .award-item .award-item-bg {
  width: 198rpx;
  height: 199rpx;
  position: absolute;
  right: 30rpx;
  top: 50%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
}
.content .award-item .award-name {
  font-size: 44rpx;
  color: #000;
  position: relative;
  z-index: 111;
  font-weight: 600;
}
.content .award-item .award-info {
  position: relative;
  z-index: 66;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 24rpx;
  line-height: 70rpx;
}
.content .award-item .award-info .award-redbags-num {
  -webkit-box-flex: 0;
  -webkit-flex: 0 0 400rpx;
          flex: 0 0 400rpx;
  white-space: nowrap;
  border-bottom: 1rpx dashed #eee;
}
.content .award-item .award-info .award-time {
  color: #999;
  line-height: 40rpx;
}
.content .award-item .award-item-bottom {
  position: relative;
  z-index: 66;
  margin-top: 10rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 20rpx;
}
.content .award-item .award-item-bottom .award-item-info {
  font-size: 26rpx;
}
.content .award-item .award-item-bottom image {
  width: 214rpx;
  height: 82rpx;
}
.content .award-item .award-item-bottom .award-code-info text {
  font-size: 26rpx;
  color: #999;
  display: block;
}
.content .award-item .award-item-bottom .award-code-info>view {
  font-weight: 600;
  color: #ed4b32;
  font-size: 32rpx;
}
.content .no-data {
  text-align: center;
  padding: 100rpx 0;
}
.content .no-data image {
  width: 374rpx;
  height: 406rpx;
  margin: 20rpx auto;
}
.content .no-data .no-data-info {
  font-size: 30rpx;
  color: #999;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.content .no-data .no-data-info navigator {
  color: #2d2793;
  text-decoration: underline;
}
.popup-wrapper {
  width: 600rpx;
  background-color: #fff;
  border-radius: 20rpx;
  padding: 50rpx;
}
.popup-wrapper .popup-qq-title {
  font-size: 36rpx;
  color: #999;
  text-align: left;
}
.popup-wrapper .popup-qq-input {
  margin: 20rpx 0;
}
.popup-wrapper .popup-qq-input input {
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  padding-left: 10rpx;
  border: 1rpx solid #999;
  border-radius: 20rpx;
  font-size: 36rpx;
  text-align: left;
}
.popup-wrapper .popup-qq-sub {
  width: 230rpx;
  height: 80rpx;
  margin: 0 auto;
}
.popup-wrapper .popup-qq-sub image {
  height: 100%;
}
.award-rules {
  border: 5rpx solid #ffde7f;
  border-radius: 15rpx;
  padding: 25rpx;
}
.award-rules .award-rules-wrapper {
  border: 1rpx solid #f5daaa;
  border-radius: 15rpx;
  padding: 35rpx;
}
.award-rules .award-rules-wrapper .award-rules-title {
  width: 254rpx;
  height: 34rpx;
  margin: 0 auto;
}
.award-rules .award-rules-wrapper .award-rules-title image {
  height: 100%;
}
.award-rules-list {
  font-size: 26rpx;
  margin-top: 30rpx;
  color: #333;
  line-height: 45rpx;
}
.award-rules-list text {
  color: #ed4b32;
  font-size: 28rpx;
}

