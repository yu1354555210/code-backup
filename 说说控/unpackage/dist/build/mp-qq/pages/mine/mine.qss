.content{padding:30rpx}.content image{width:100%;height:100%}.mine-info{display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-align:center;-webkit-align-items:center;align-items:center;margin-bottom:30rpx}.mine-info .avatar{width:120rpx;height:120rpx;margin-right:20rpx;border-radius:50%;overflow:hidden}.mine-info .nickname{font-size:34rpx}.mine-list{margin-top:40rpx}.mine-list .mine-item{width:100%;background-color:#f6f6f6;height:90rpx;padding:0 30rpx;margin-bottom:30rpx;font-size:28rpx;border-radius:20rpx;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-align:center;-webkit-align-items:center;align-items:center;-webkit-box-pack:justify;-webkit-justify-content:space-between;justify-content:space-between}.mine-list .mine-item .mine-item-left{display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-align:center;-webkit-align-items:center;align-items:center}.mine-list .mine-item .mine-item-left .mine-icon{width:40rpx;height:40rpx;margin-right:20rpx}.mine-list .mine-item .arrow{width:15rpx;height:26rpx}.banner-box{margin:30rpx 0}.login-box{display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;margin-top:130rpx}.login-box .login-btn{width:400rpx;height:80rpx;border-radius:200rpx;border:none;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;-webkit-box-align:center;-webkit-align-items:center;align-items:center;background:-webkit-linear-gradient(right,#999,#cacaca);background:linear-gradient(-90deg,#999,#cacaca);font-size:30rpx;font-weight:700;color:#fff}.login-box .login-btn::after{display:none}.login-box .login-btn.login-before{background:-webkit-linear-gradient(left,#ffce00,#ffb400);background:linear-gradient(90deg,#ffce00,#ffb400)}